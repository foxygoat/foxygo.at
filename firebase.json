{
   "hosting": {
      "ignore": [
         "firebase.json",
         "**/.*"
      ],
      "public": "public",
      "redirects": [
         {
            "destination": "https://raw.githubusercontent.com/foxygoat/jsonnext/:path",
            "source": "jsonnext/:path*",
            "type": 302
         }
      ]
   }
}
